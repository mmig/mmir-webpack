{
  "name": "mmir-webpack",
  "version": "5.0.0",
  "keywords": [
    "mmir",
    "multimodal",
    "dialog",
    "system",
    "speech",
    "asr",
    "tts"
  ],
  "author": "russaa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmig/mmir-webpack/issues"
  },
  "homepage": "https://github.com/mmig/mmir-webpack#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmig/mmir-webpack.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "lib": "./dist"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/loader-utils": "^1.1.3",
    "@types/webpack": "^4.32.1",
    "cross-var": "^1.1.0",
    "fs-extra": "^8.1.0",
    "mmir-lib": "git+https://github.com/mmig/mmir-lib.git",
    "typedoc": "^0.15.0",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "webpack-dev-server",
    "doc": "npm run clean-doc && npm run gen-doc && npm run gh-doc",
    "clean-doc": "node -e require('fs-extra').emptyDirSync('docs')",
    "gen-doc": "cross-var typedoc --name \"$npm_package_name $npm_package_version\" index.d.ts",
    "gh-doc": "node -e require('fs-extra').ensureFileSync('docs/.nojekyll')"
  },
  "dependencies": {
    "file-loader": "^2.0.0",
    "lodash": "^4.17.11",
    "mmir-tooling": "git+https://github.com/mmig/mmir-tooling.git",
    "raw-loader": "^0.5.1",
    "val-loader": "^1.1.1",
    "virtual-module-webpack-plugin": "^0.4.1",
    "worker-loader": "^2.0.0"
  }
}
