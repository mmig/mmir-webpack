{
  "name": "mmir-webpack",
  "version": "6.1.1",
  "description": "Integration for MMIR library in webpack builds",
  "keywords": [
    "mmir",
    "webpack"
  ],
  "author": "russaa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmig/mmir-webpack/issues"
  },
  "homepage": "https://github.com/mmig/mmir-webpack#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmig/mmir-webpack.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "lib": "./dist"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/loader-utils": "^1.1.3",
    "@types/webpack": "^4.41.7",
    "cross-var": "^1.1.0",
    "eclint": "^2.8.1",
    "file-loader": ">=4.3.0",
    "fs-extra": "^8.1.0",
    "mmir-lib": "^6.1.0",
    "raw-loader": ">=3.1.0",
    "typedoc": "^0.16.11",
    "typedoc-plugin-external-module-name": "^3.0.0",
    "typedoc-plugin-markdown": "^2.2.17",
    "val-loader": ">=1.1.1",
    "virtual-module-webpack-plugin": "^0.4.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "worker-loader": "^2.0.0"
  },
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "webpack-dev-server",
    "doc": "npm run clean-doc && npm run gen-doc",
    "clean-doc": "node -e require('fs-extra').emptyDirSync('docs')",
    "gen-doc": "cross-var typedoc --name \"$npm_package_name $npm_package_version\" index.d.ts"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "mmir-tooling": "^6.1.1"
  },
  "peerDependencies": {
    "mmir-lib": "^6.1.0",
    "webpack": ">=3.0.0",
    "file-loader": ">=3.0.0",
    "raw-loader": ">=3.0.0",
    "virtual-module-webpack-plugin": ">=0.4.1",
    "val-loader": ">=1.1.1",
    "worker-loader": ">=2.0.0"
  }
}