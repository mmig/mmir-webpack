{
  "name": "mmir-webpack",
  "version": "6.1.1",
  "description": "Integration for MMIR library in webpack builds",
  "keywords": [
    "mmir",
    "webpack"
  ],
  "author": "russaa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmig/mmir-webpack/issues"
  },
  "homepage": "https://github.com/mmig/mmir-webpack#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmig/mmir-webpack.git"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "lib": "./dist"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.5",
    "@types/loader-utils": "^2.0.1",
    "@types/webpack": "^4.41.25",
    "cross-var": "^1.1.0",
    "eclint": "^2.8.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.0.1",
    "jquery": "^3.5.1",
    "mmir-lib": "^6.2.0",
    "raw-loader": "^4.0.2",
    "typedoc": "^0.19.2",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typedoc-plugin-markdown": "^3.1.1",
    "typescript": "^4.1.3",
    "val-loader": "^2.1.2",
    "virtual-module-webpack-plugin": "^0.4.1",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^3.0.6"
  },
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "webpack-dev-server",
    "doc": "npm run clean-doc && npm run gen-doc",
    "clean-doc": "node -e require('fs-extra').emptyDirSync('docs')",
    "gen-doc": "cross-var typedoc --name \"$npm_package_name $npm_package_version\" index.d.ts"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "mmir-tooling": "^6.2.1"
  },
  "peerDependencies": {
    "mmir-lib": "^6.2.0",
    "webpack": ">=3.0.0",
    "file-loader": ">=3.0.0",
    "raw-loader": ">=3.0.0",
    "virtual-module-webpack-plugin": ">=0.4.1",
    "val-loader": ">=1.1.1",
    "worker-loader": ">=2.0.0"
  }
}
